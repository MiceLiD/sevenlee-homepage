    "dotenv": "^6.0.0",
    "handlebars": "^4.0.11",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-handlebars": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.2",
    "koa-views": "^6.1.4",
    "mini-logger": "^1.1.3",
    "mysql2": "^1.6.1",
    "node-fetch": "^2.1.2",
    "rc-texty": "^0.1.1",
